# Normalize line endings of text files
* text=auto

# Windows files should have CRLF
*.bat text eol=crlf
*.cmd text eol=crlf
*.ps1 text eol=crlf

# There's more (see <https://www.git-scm.com/docs/gitattributes>) but just
# including the more common ones
*.css   diff=css
*.go    diff=golang
*.htm?  diff=html
*.pl    diff=perl
*.php   diff=php
*.phtml diff=php
*.py    diff=python
*.rb    diff=ruby

# Exclude these files from export
.gitattributes export-ignore
.gitignore export-ignore
.github export-ignore
